{
  "name":"mytest",
  "version":"1.0.0",
  "description": "copy link as markdown only for asana",
  "content_scripts": [{
    "matches": ["https://app.asana.com/*"],
    "js": ["content.js"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "mytest"
  },
  "permissions": [
   "activeTab", "tabs", "clipboardWrite", "clipboardRead"
  ],
  "manifest_version":2
}
